var
  matPM: Tmatrix;
  bmPM: TVSbitmap;
  
  VfuncPM:Tvector; // contient la valeur de alpha en fonction de la distance au centre
                   // 1 au milieu , 0  la priphrie
  dxPM, dyPM, dx0PM: real; 
  
procedure InitFuncPM(d,b:real);
var
  i:integer;
begin
  with VfuncPM do
  begin
    create(t_single,0,1000);
    Yvalue[0]:=1;
    for i:=1 to Iend do
      Yvalue[i]:=1/(1+exp(b*ln(i/d)));
  end; 
end;

procedure initMaskPM;
var
  i,j:integer;
  d,zz:real;
const
  N=128;
begin
  bmPM.create;
    
  matPM.create(t_single,0,N-1,0,N-1);
  matPM.fill(stimScreen.backGroundLum);                     // 50: valeur arbitraire
  bmPM.loadFromMatrix(matPM,true);    // indispensable pour dimensionner le bm
    
  initFuncPM(N/4,10);                 // pour d=125, on aura alpha= 0.5
  with matPM do
  begin
    for i:= Istart to Iend do
    for j:= Jstart to Jend do
    begin         
      d:=sqrt( sqr(i-N div 2)+sqr(j-N div 2));
      matPM[i,j]:= 1-VfuncPM.Rvalue[d];
    end;  
  end;
    
  with bmPM do
  begin
    loadAlphaFromMatrix(matPM,true);
    dx:=dxPM;
    dy:=dxPM;
    imageWidth:=dx0PM;                   
    imageHeight:=dx0PM;                 // on peut dimensionner le bm comme on veut
    Zdistance:=10; 
    TileMode:=false;
    setBorderLum(stimScreen.BackGroundLum,1);
  end; 


  //bmPM.Fmask:=true;
  bmPM.Onscreen:=true;

end;  

