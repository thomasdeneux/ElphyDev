var 
  stm:TVSmovie; 
 
 
procedure InitVSmovie; 
var 
  i,n:integer; 
begin 
  for i:=0 to 100 do 
  begin 
    with bar[i] do 
    begin 
      create(''); 
      dx:=0.5+i*0.1; 
      dy:=0.5+i*0.1; 
      theta:=i; 
      lum:=25; 
    end; 
  end; 
 
  with stm do 
  begin 
    create(''); 
 
    cycleCount:=10; 
 
    for i:=0 to 100 do 
      addVisualObject(bar[i]); 
 
    for i:=0 to 99 do 
    begin 
      n:=round(50+50*sin(2*pi/100*i)); 
      addFrame(10*cos(2*pi/100*i),10*sin(2*pi/100*i),n); 
    end; 
  end; 
end; 
 
