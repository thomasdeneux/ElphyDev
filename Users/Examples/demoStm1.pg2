procedure InitVSmovieGab; 
var 
  i,n:integer; 
begin 
  for i:=0 to 99 do 
  begin 
    with gabor[i] do 
    begin 
      create(''); 
      dx:=15; 
      dy:=15; 
      theta:=60; 
      Lx:=3; 
      Ly:=3; 
      contrast:=1; 
 
      phase:=360*i/100; 
 
    end; 
  end; 
 
  with stmGabor do 
  begin 
    create(''); 
 
    cycleCount:=10; 
 
    for i:=0 to 99 do 
      addVisualObject(gabor[i]); 
 
    for i:=0 to 99 do 
    begin 
      addFrame(0,0,i); 
    end; 
  end; 
end; 
 
