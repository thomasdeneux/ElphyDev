
var
  bar:array[0..1000] of Tbar;
  stim:array[1..1000] of Ttranslation;
  
  grat:array[1..1000] of TLgrating;
  tr:array[1..1000] of TphaseTranslation;

var
  Transform: TVSpolarToCart;
                                                  

procedure DisableStims;
begin
  VisualStim.DisableStims;
  if assigned(transform) then transform.active:=true;
end;
 
procedure clearAll; 
var 
  i:integer;                         
begin 
  for i:=1 to 1000 do 
  begin                              
    bar[i].free; 
    stim[i].free;                                     
    grat[i].free; 
    tr[i].free;                            
  end;                                     
end; 

procedure initBarres; 
var  
  i:integer;                                         
begin 
  for i:=1 to 400 do 
  begin 
    with bar[i] do 
    begin 
      create;                                        
      dx:=0.1+random*5; 
      dy:=0.2; 
      lum:=random*100;
      //rgbColor:=rgb(randomI(256),randomI(256),randomI(256)); 
    end; 
 
    with stim[i] do 
    begin 
      create; 
      dtON:=0.1+random; 
      cycleCount:=10; 
      orthogonal:=true; 
      backandforth:=true; 
      x0:=(random-0.5)*20; 
      y0:=(random-0.5)*15; 
      v0:=1+random*10; 
      theta0:=180*(random-0.5); 
      startTime:=random*2;
      setVisualObject(bar[i]); 
    end; 
  end; 
end; 
 
program 'Barres' 
initchrono;
DisableStims; 
initBarres; 
VisualStim.animate; 
statuslineTxt(chrono); 
 
 
procedure initB(num,x1,y1,cote:integer;delta:real); 
var 
  i:integer; 
begin 
 
  for i:=0 to 3 do 
  begin 
    with bar[(num-1)*4+i+1] do 
    begin 
      create(''); 
      dx:=1.5; 
      dy:=0.2; 
      x:=x1; 
      y:=y1; 
    end; 
 
    with stim[(num-1)*4+i+1] do 
    begin 
      create(''); 
 
      cycleCount:=1; 
      orthogonal:=true; 
 
      v0:=10; 
      dtON:=cote/v0; 
      dtOff:=dtON*3; 
      startTime:=i*dtON+delta; 
 
      case i of 
      0:begin 
         x0:=x1; 
         y0:=y1; 
         theta0:=0; 
        end; 
      1:begin 
         x0:=x1+cote; 
         y0:=y1; 
         theta0:=-90; 
        end; 
      2:begin 
         x0:=x1+cote; 
         y0:=y1-cote; 
         theta0:=180; 
        end; 
      3:begin 
         x0:=x1; 
         y0:=y1-cote; 
         theta0:=90; 
        end; 
      end; 
 
      setVisualObject(bar[(num-1)*4+i+1]); 
 
    end; 
  end; 
end; 
 
procedure initBarres2; 
var 
  i:integer; 
begin 
  for i:=1 to 25 do initB(i,-10,10,15,0.2*(i-1)); 
end; 
 
program 'Barres 1' 
disableStims; 
initBarres2; 
VisualStim.animate; 
 
 
 
 
