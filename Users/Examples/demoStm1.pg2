procedure initGrat3; 
var 
  i:integer; 
begin 
  with grat[1] do 
  begin 
    create(''); 
    dx:=StimScreen.Width; 
    dy:=StimScreen.Height;
    orientation:=-30;   
    period:=5; 
    BlendAlpha:=0.5;
    OnScreen:=false;
    //squareWave:=true;
  end; 
  
  with grat[2] do 
  begin 
    create(''); 
    dx:=StimScreen.Width; 
    dy:=StimScreen.Height; 
    orientation:=60; 
    period:=8;
    Zdistance:=-1;
    squareWave:=true; 
    //BlendAlpha:=0.5;
  end; 
  
  for i:=1 to 2 do
  with tr[i] do 
  begin 
    create(''); 
    dtON:=5; 
    dtOff:=0;
    CycleCount:=1;
    speed:=200+(i-1)*100; 
    setVisualObject(grat[i]); 
  end; 
  visualStim.extraTime:=2;
   
end; 
 
 
