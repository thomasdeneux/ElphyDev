Const
  maxSpot1=100;
  maxSpot2=300;
  
var
  spot1: array[1..MaxSpot1] of Tbar;
  spot2: array[1..MaxSpot2] of Tbar;

  SpotTraj1: array[1..maxSpot1] of Ttrajectory; 
  SpotTraj2: array[1..maxSpot2] of Ttrajectory;
  xyPlot: TxyPlot;
    
    
procedure initSpots; 
var  
  i:integer; 
begin 
  with xyPlot do
  begin
    create;
    addPolyline;
    with last do
    for i:=0 to 100 do 
      addpoint(10*cos(2*pi/100*i),10*sin(2*pi/100*i));
  end;

  for i:=1 to maxSpot1 do 
  begin 
    with spot1[i] do 
    begin 
      create; 
      dx:=0.1; 
      dy:=0.1; 
      lum:=100; 
    end; 
 
    with SpotTraj1[i] do 
    begin 
      create;
      setVisualObject(spot1[i]); 
      ParticleFlow(1000,1000,10,30,10,xyplot);    
    end; 
  end; 
 
  
  for i:=1 to maxSpot2 do 
  begin 
    with spot2[i] do 
    begin 
      create; 
      dx:=0.1; 
      dy:=0.1; 
      lum:=100; 
    end;     
 
    with SpotTraj2[i] do
    begin
      create;
      setVisualObject(spot2[i]); 
      BrownianMvt((random-0.5)*2000,(random-0.5)*2000,10,5,10,xyplot); 
    end; 
  end; 
          
end; 
 
