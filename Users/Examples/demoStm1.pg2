procedure initGrat2; 
var 
  i,j:integer;
  dx1,dy1:real; 
  Nx,Ny:integer;
begin 
  Nx:=24;
  Ny:=12;

  dx1:= (StimScreen.Width-6) / Nx;  
  dy1:= StimScreen.Height / Ny; 
  
  for i:=0 to Nx-1 do 
  for j:=0 to Ny-1 do 
  begin 
    with grat[1+i*Ny+j] do 
    begin 
      create(''); 
      x:=-(StimScreen.Width-6)/2 + dx1/2 + i*dx1; 
      y:= -StimScreen.Height/2 + dy1/2 + j*dy1; 
      dx:= dx1*0.9;  
      dy:= dy1*0.9; 
      period:=0.5+0.5*i; 
      elliptic:= true; 
    end; 
 
    with tr[1+i*Ny+j] do 
    begin 
      create(''); 
      dtON:=10; 
      speed:=200+j*200; 
      setVisualObject(grat[1+i*Ny+j]); 
      setsyncpulse(5); 
      starttime:=0; {random*2;} 
    end; 
  end; 
end; 
 
